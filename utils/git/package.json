{
  "name": "want-js-utils.git",
  "version": "0.0.2",
  "description": "Git utils for use in want-js",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "find . -type f -name \"*.d.ts\" -delete -o -name \"*.js\" -delete -o -name \"*.js.map\" -delete",
    "prepare": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/want-js/want-js.git"
  },
  "author": {
    "name": "Evgeniy Shershnev",
    "email": "joshuan.chel@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/want-js/want-js/issues"
  },
  "homepage": "https://github.com/want-js/want-js/tree/master/utils/git#readme",
  "dependencies": {
    "debug": "4.1.1",
    "git-remote-origin-url": "3.0.0",
    "simple-git": "1.126.0",
    "typescript": "3.7.2"
  },
  "devDependencies": {
    "@types/debug": "4.1.5"
  },
  "files": [
    "lib/*.js",
    "lib/*.js.map",
    "lib/*.d.ts"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
